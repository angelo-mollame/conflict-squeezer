{
    "name": "conflict-squeezer",
    "displayName": "Conflict Squeezer",
    "description": "Simplifies the resolution of Git merge conflicts",
    "version": "0.0.1",
    "publisher": "angelo-mollame",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:conflictSqueezer.squeezeConflicts"
    ],
    "main": "./out/Extension",
    "contributes": {
        "commands": [{
            "command": "conflictSqueezer.squeezeConflicts",
            "title": "Squeeze Conflicts"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}